#!/bin/sh

linenum=0
fieldnum=0

store()
{
while read line
do
lines[$linenum]="$line"
linenum=$(expr "$linenum" + 1)
fieldnumtmp=$(echo "$line" | awk '{print NF}')
fieldnum=$([ "$fieldnumtmp" -gt "$fieldnum" ] && echo "$fieldnumtmp" || echo "$fieldnum" )
done
}

flow()
{
curline=0

while [ "$curline" -lt "$linenum" ]
do
echo ${lines[$curline]}
curline=$(expr "$curline" + 1)
done
}
